body {
    font-family: 'Arial', sans-serif; 
    line-height: 1.6;
    margin: 0;
    padding: 0;
    background-color: #f9f9f9;
}


.hero {

    color: #fff;
    padding: 80px 0;
    text-align: left;
}

.hero h1 {
    font-size: 2.5rem;
    margin-bottom: 20px;
}

.hero p {
    font-size: 1.2rem;
}

/* Buttons */
.btn {
    padding: 10px 20px;
    border: none;
    border-radius: 5px;
    font-size: 1rem;
    cursor: pointer;
}

.btn-secondary {
    background-color: #ff5722; /* Secondary button color */
    color: #fff;
}

.btn-white-outline {
    background-color: transparent;
    border: 2px solid #ff5722; /* Outline color */
    color: #ff5722;
}

/* Product Section */
.product-section {
    padding: 60px 0;
}

.product-section .section-title {
    font-size: 2rem;
    margin-bottom: 30px;
}

.product-item {
    text-align: center;
    padding: 15px;
    border: 1px solid #ddd; /* Light border for product items */
    border-radius: 5px;
    background-color: #fff; /* White background for product items */
    transition: transform 0.3s;
}

.product-item:hover {
    transform: scale(1.05); /* Slight zoom on hover */
}

.product-title {
    font-size: 1.5rem;
    margin: 10px 0;
}

/* Why Choose Us Section */
.why-choose-section {
    background-color: #e3f2fd; /* Light blue background */
    padding: 60px 0;
}

.feature {
    text-align: center;
    padding: 20px;
}

.feature .icon {
    margin-bottom: 15px;
}

/* We Help Section */
.we-help-section {
    padding: 60px 0;
}

/* Popular Products Section */
.popular-product {
    padding: 60px 0;
}

.product-item-sm {
    display: flex;
    align-items: center;
    background-color: #fff;
    border: 1px solid #ddd;
    border-radius: 5px;
    padding: 10px;
    margin-bottom: 20px;
}

/* Blog Section */
.blog-section {
    padding: 60px 0;
}

.post-entry {
    background-color: #fff;
    border: 1px solid #ddd;
    border-radius: 5px;
    margin-bottom: 20px;
}

/* Footer Section */
.footer-section {
    
    color: #fff;
    
}

.footer-logo {
    font-size: 1.5rem;
    color: #ff5722; /* Logo color */
}

.footer-logo span {
    color: #fff; /* Logo text color */
}

/* Subscription Form */
.subscription-form {
    display: flex;
    align-items: center;
}

.subscription-form input[type="text"],
.subscription-form input[type="email"] {
    margin-right: 10px;
    border: 1px solid #ddd;
    border-radius: 5px;
    padding: 10px;
}

/* Responsive Styles */
@media (max-width: 768px) {
    .hero {
        text-align: center; /* Center align text on mobile */
    }

    .hero h1 {
        font-size: 2rem; /* Adjust font size for mobile */
    }
}

</style>
